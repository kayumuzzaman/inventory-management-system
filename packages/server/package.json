{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon src/index.ts",
    "prod": "export NODE_ENV=production && nodemon src/index.ts",
    "precommit": "yarn lint && lint-staged",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "yarn run lint -- --fix"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/morgan": "^1.9.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lint": "^0.7.0",
    "lint-staged": "^13.0.3",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "ts-node": "^10.9.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix './**/*.ts'"
    ]
  }
}
